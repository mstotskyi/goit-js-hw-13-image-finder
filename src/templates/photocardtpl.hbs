{{#each this}}
<li class="gallery__item">
  <a  href="" class="link gallery__link">
    <div class="photo-card">
    <img class="gallery__image" src="{{webformatURL}}" width="290" data-source="{{largeImageURL}}" alt="{{tags}}" id="{{id}}"/>

  <div class="stats">
    <p class="stats-item">
      <i class="material-icons">thumb_up</i>
      {{likes}}

    </p>
    <p class="stats-item">
      <i class="material-icons">visibility</i>
      {{views}}

    </p>
    <p class="stats-item">
      <i class="material-icons">comment</i>
      {{comments}}
      
    </p>
    <p class="stats-item">
      <i class="material-icons">cloud_download</i>
      {{downloads}}
    </p>
  </div>
</div>
  </a>
</li>

{{/each}}